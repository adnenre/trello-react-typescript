{
    "name": "trello-react-typescript",
    "version": "0.2.2",
    "homepage": ".",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@mui/icons-material": "^5.6.2",
        "@mui/lab": "^5.0.0-alpha.78",
        "@mui/material": "^5.6.1",
        "@mui/styled-engine-sc": "^5.1.0",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.20.36",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@types/react-fontawesome": "^1.6.5",
        "axios": "^0.24.0",
        "chroma-js": "^2.4.2",
        "concurrently": "^6.4.0",
        "react": "^17.0.2",
        "react-dnd": "^14.0.1",
        "react-dnd-html5-backend": "^14.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.2.1",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.0",
        "styled-components": "^5.3.3",
        "typescript": "^4.4.4",
        "use-immer": "^0.6.0",
        "use-storage-node": "^2.0.0",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "postInstall": "( npm install ) && ( cd server && npm install )",
        "dev": "cross-env REACT_APP_API='DEV_URL' concurrently \"react-scripts start\" \"cd server && ts-node-dev src/index.ts\"",
        "start": "cross-env REACT_APP_API='PROD_URL' react-scripts start",
        "start:prod": "cross-env REACT_APP_API='PROD_URL' react-scripts build && serve -s build -l 4000 ",
        "build": "cross-env REACT_APP_API='PROD_URL' react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "src/theme/**/**/*.js"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.15",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "gh-pages": "^3.2.3",
        "jsonwebtoken": "^8.5.1",
        "prettier": "^2.8.1",
        "pretty-quick": "^3.1.3"
    }
}
